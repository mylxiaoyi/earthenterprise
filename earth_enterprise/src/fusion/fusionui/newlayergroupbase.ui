  <exportmacro></exportmacro>
  <class>NewLayerGroupBase</class>
  <widget class="QDialog" name="NewLayerGroupBase">
    <property name="geometry">
      <rect>
        <x>0</x>
        <y>0</y>
        <width>486</width>
        <height>158</height>
      </rect>
    </property>
    <property name="windowTitle">
      <string>New Layer Group</string>
    </property>
    <layout class="QGridLayout">
      <item row="2" column="0">
        <widget class="Q3GroupBox" name="GroupBox1">
          <property name="title">
            <string>Properties</string>
          </property>
          <layout class="QGridLayout">
            <property name="margin">
              <number>11</number>
            </property>
            <property name="spacing">
              <number>6</number>
            </property>
            <item row="0" column="0">
              <widget class="QLabel" name="TextLabel3">
                <property name="text">
                  <string>Default State:</string>
                </property>
                <property name="alignment">
                  <set>Qt::AlignVCenter|Qt::AlignRight</set>
                </property>
                <property name="buddy" stdset="0">
                  <cstring>stateCombo</cstring>
                </property>
                <property name="wordWrap">
                  <bool>false</bool>
                </property>
              </widget>
            </item>
            <item row="0" column="1">
              <widget class="QComboBox" name="stateCombo">
                <property name="toolTip" stdset="0">
                  <string>Initial visibility state of this layer for client</string>
                </property>
                <item>
                  <property name="text">
                    <string>Off</string>
                  </property>
                </item>
                <item>
                  <property name="text">
                    <string>On</string>
                  </property>
                </item>
              </widget>
            </item>
            <item row="0" column="2">
              <spacer name="Spacer14">
                <property name="sizeHint">
                  <size>
                    <width>55</width>
                    <height>16</height>
                  </size>
                </property>
                <property name="sizeType">
                  <enum>QSizePolicy::Expanding</enum>
                </property>
                <property name="orientation">
                  <enum>Qt::Horizontal</enum>
                </property>
              </spacer>
            </item>
          </layout>
        </widget>
      </item>
      <item row="3" column="0">
        <layout class="QHBoxLayout">
          <property name="margin">
            <number>0</number>
          </property>
          <property name="spacing">
            <number>6</number>
          </property>
          <item>
            <spacer name="Horizontal Spacing2_3">
              <property name="sizeHint">
                <size>
                  <width>20</width>
                  <height>0</height>
                </size>
              </property>
              <property name="sizeType">
                <enum>QSizePolicy::Expanding</enum>
              </property>
              <property name="orientation">
                <enum>Qt::Horizontal</enum>
              </property>
            </spacer>
          </item>
          <item>
            <widget class="QPushButton" name="okBtn">
              <property name="enabled">
                <bool>false</bool>
              </property>
              <property name="text">
                <string>OK</string>
              </property>
              <property name="shortcut">
                <string/>
              </property>
              <property name="autoDefault">
                <bool>true</bool>
              </property>
              <property name="default">
                <bool>true</bool>
              </property>
            </widget>
          </item>
          <item>
            <widget class="QPushButton" name="cancelBtn">
              <property name="text">
                <string>Cancel</string>
              </property>
              <property name="shortcut">
                <string/>
              </property>
              <property name="autoDefault">
                <bool>true</bool>
              </property>
              <property name="default">
                <bool>true</bool>
              </property>
            </widget>
          </item>
        </layout>
      </item>
      <item row="1" column="0">
        <layout class="QHBoxLayout">
          <item>
            <widget class="QLabel" name="TextLabel1">
              <property name="text">
                <string>Name:</string>
              </property>
              <property name="alignment">
                <set>Qt::AlignVCenter|Qt::AlignRight</set>
              </property>
              <property name="buddy" stdset="0">
                <cstring>layerName</cstring>
              </property>
              <property name="wordWrap">
                <bool>false</bool>
              </property>
            </widget>
          </item>
          <item>
            <widget class="QLineEdit" name="layerName"/>
          </item>
        </layout>
      </item>
    </layout>
  </widget>
  <layoutdefault spacing="6" margin="11"/>
  <pixmapfunction>qPixmapFromMimeSource</pixmapfunction>
  <tabstops>
    <tabstop>layerName</tabstop>
    <tabstop>stateCombo</tabstop>
    <tabstop>okBtn</tabstop>
    <tabstop>cancelBtn</tabstop>
  </tabstops>
  <connections>
    <connection>
      <sender>cancelBtn</sender>
      <signal>released()</signal>
      <receiver>NewLayerGroupBase</receiver>
      <slot>reject()</slot>
    </connection>
    <connection>
      <sender>okBtn</sender>
      <signal>released()</signal>
      <receiver>NewLayerGroupBase</receiver>
      <slot>accept()</slot>
    </connection>
    <connection>
      <sender>layerName</sender>
      <signal>textChanged(QString)</signal>
      <receiver>NewLayerGroupBase</receiver>
      <slot>changeName(QString)</slot>
    </connection>
  </connections>
</ui>
