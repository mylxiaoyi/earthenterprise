    <connection>
      <sender>accept_btn</sender>
      <signal>clicked()</signal>
      <receiver>BalloonStyleTextBase</receiver>
      <slot>accept()</slot>
    </connection>
    <connection>
      <sender>cancel_btn</sender>
      <signal>clicked()</signal>
      <receiver>BalloonStyleTextBase</receiver>
      <slot>reject()</slot>
    </connection>
    <connection>
      <sender>insert_default_btn</sender>
      <signal>clicked()</signal>
      <receiver>BalloonStyleTextBase</receiver>
      <slot>InsertDefault()</slot>
