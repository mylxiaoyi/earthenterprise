          <property name="minimumSize">
            <size>
              <width>460</width>
              <height>0</height>
            </size>
          </property>
          <property name="text">
            <string/>
          </property>
        </widget>
      </item>
      <item row="0" column="1">
        <widget class="QPushButton" name="insert_default_btn">
          <property name="text">
            <string>Insert Default Text</string>
